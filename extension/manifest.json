{
    "name": "Cross Reference Assist",
    "description": "Use ChatGPT to connect scripture references by topic",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Cross reference with ChatGPT"
      },
    "permissions": ["activeTab", "scripting"],
    "icons": {
        "72": "icon_72.png",
        "80": "icon_80.png",
        "128": "icon_128.png"
     }

}